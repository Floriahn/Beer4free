
public class File implements IComponent{

	String name;
	int size;
	
	public File(String name, int size){
		this.name = name;
		this.size = size;
	}
	
	@Override
	public void getHirarchy() {
		System.out.println("|\t"+name);
	}

	@Override
	public int getSize() {
		return size;
	}
	
	public String getName(){
		return name;
	}

}
